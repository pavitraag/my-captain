#include <stdio.h>
int lcm (int ,int);
int main()
{
	int a,b,check;
	int prime[100];
	
	printf("enter two numbers \n");
	scanf("%d %d",&a ,&b);
	check = lcm (a,b);
		printf("lcm of %d and %d is %d\n",a,b,check);
}
int lcm(int a,int b)
{
	static int common =1;
	if(common%a==0 && common%b==0)
	{return common;
	}
	common++;
	lcm(a,b);
	return common;
}
